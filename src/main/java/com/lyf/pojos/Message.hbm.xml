<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.lyf.pojos.Message" table="message" schema="personmanage">
        <id name="mid" column="mid"/>
        <property name="mtitle" column="mtitle"/>
        <property name="mcontent" column="mcontent"/>
        <property name="mdate" column="mdate"/>
        <property name="aid" column="aid"/>
        <property name="username" column="username"/>
    </class>
</hibernate-mapping>