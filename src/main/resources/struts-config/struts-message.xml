<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="message" extends="struts-default" namespace="/message">
        <action name="messageAction_messageList" class="com.lyf.action.MessageAction" method="messageList">
            <result name="messageList">/WEB-INF/msg_manage.jsp</result>
        </action>
        <action name="messageAction_queryMessage" class="com.lyf.action.MessageAction" method="queryMessage">
            <result name="queryMessage">/WEB-INF/msg_look.jsp</result>
        </action>
        <action name="messageAction_deleteMessage" class="com.lyf.action.MessageAction" method="deleteMessage">
            <result name="messageList">/WEB-INF/msg_manage.jsp</result>
        </action>
        <action name="messageAction_towardMessageSend" class="com.lyf.action.MessageAction" method="towardMessageSend">
            <result name="towardMessageSend">/WEB-INF/msg_send.jsp</result>
        </action>
        <action name="messageAction_messageSend" class="com.lyf.action.MessageAction" method="messageSend">
            <result name="messageList">/WEB-INF/msg_manage.jsp</result>
        </action>
    </package>
</struts>